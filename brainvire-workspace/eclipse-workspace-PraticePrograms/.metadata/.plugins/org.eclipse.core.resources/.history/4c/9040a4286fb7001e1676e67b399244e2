public class P135_LinkedList {
	Node head;
	private int size;

	public P135_LinkedList() {
		this.size = 0;
	}

	class Node {
		String data;
		Node next;

		Node(String data) {
			this.data = data;
			this.next = null;
			size++;
		}
		
		public void addFirstLoc(String data) {
			Node newnode = new Node(data);
		}
	}

